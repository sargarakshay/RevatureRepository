<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>servletConfigAndservletContext</display-name>
    <servlet>
        <servlet-name>ServletConfig</servlet-name>
        <servlet-class>com.app.servlet.ServletConfigDemo</servlet-class>
        <init-param>
            <param-name>playerName</param-name>
            <param-value>Akshay</param-value>
        </init-param>
        <init-param>
            <param-name>playerAge</param-name>
            <param-value>21</param-value>
        </init-param>
        <init-param>
            <param-name>playerGame</param-name>
            <param-value>Coding</param-value>
        </init-param>
        <init-param>
            <param-name>playerCity</param-name>
            <param-value>Navi Mumbai</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletConfig</servlet-name>
        <url-pattern>/config</url-pattern>
    </servlet-mapping>
</web-app>