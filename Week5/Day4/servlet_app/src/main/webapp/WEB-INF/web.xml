<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>servlet_app</display-name>
<servlet>
    <display-name>HelloServlet</display-name>
    <servlet-name>Servlet</servlet-name>
    <servlet-class>com.app.servlet_app.Servlet</servlet-class>
    <init-param>
        <param-name>english</param-name>
        <param-value>89</param-value>
    </init-param>
    <init-param>
        <param-name>marathi</param-name>
        <param-value>100</param-value>
    </init-param>
    <init-param>
        <param-name>hindi</param-name>
        <param-value>99</param-value>
    </init-param>
</servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>midsem</param-name>
        <param-value>9.99</param-value>
    </context-param>
    <context-param>
        <param-name>endsem</param-name>
        <param-value>8.98</param-value>
    </context-param>
</web-app>